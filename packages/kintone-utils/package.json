  },
  "peerDependencies": {
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@kintone-plugin/eslint-config": "workspace:*",
    "@kintone-plugin/tsconfig": "workspace:*",
    "@kintone/dts-gen": "^9.0.0",
    "@types/react": "^19.2.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  }
