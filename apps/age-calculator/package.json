{
  "name": "age-calculator",
  "version": "1.0.0",
  "description": "kintone年齢計算プラグイン",
  "author": "Masanobu Sunohara",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "init": "run-s init:env init:key",
    "init:env": "node scripts/dotenv.js",
    "init:key": "node scripts/genkey.js",
    "dev": "cross-env NODE_ENV=development run-s bundle package upload",
    "build": "cross-env NODE_ENV=production run-s bundle package",
    "bundle": "rsbuild build",
    "package": "kintone-plugin-packer dist --ppk private.ppk --out artifacts/plugin.zip",
    "upload": "node scripts/upload.js",
    "deploy": "run-s build upload",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "fix": "run-p fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist artifacts .turbo"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@kintone-plugin/ui": "workspace:*",
    "@kintone/rest-api-client": "^6.1.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "jotai": "^2.16.0",
    "nanoid": "^5.1.6",
    "notistack": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.69.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@kintone-plugin/eslint-config": "workspace:*",
    "@kintone-plugin/kintone-utils": "workspace:*",
    "@kintone-plugin/tsconfig": "workspace:*",
    "@kintone/dts-gen": "^9.0.0",
    "@kintone/plugin-packer": "^9.0.0",
    "@kintone/plugin-uploader": "^10.0.0",
    "@rsbuild/core": "^1.6.15",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rsbuild/plugin-type-check": "^1.3.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "fs-extra": "^11.3.3",
    "node-rsa": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  }
}
